<!-- BEGIN_SetTemplate id="columns" -->
"columns": [
  <!-- BEGIN entry_custom_column_group:loop -->
  <!-- BEGIN entry_custom_column_group:glue -->,<!-- END entry_custom_column_group:glue -->
  {
      "id": "{entry_custom_column_id}[jsonEscape|raw|allow_dangerous_tag]",
      "label": "{entry_custom_column_label}[jsonEscape|raw|allow_dangerous_tag]",
      "accessorKey": "{entry_custom_column_accessor_key}[jsonEscape|raw|allow_dangerous_tag]",
      "type": "{entry_custom_column_type}[jsonEscape|raw|allow_dangerous_tag]",
      "enableSorting": <!-- BEGIN_IF [{entry_custom_column_sort}/eq/on] -->true<!-- ELSE -->false<!-- END_IF -->,
      <!-- BEGIN_IF [{entry_custom_column_type}/re/^(textarea|rich-editor|block-editor)$] -->
      "styles": {
        "header": {
          "minWidth": "240px"
        },
        "cell": {
          "maxWidth": "240px"
        }
      },
      <!-- END_IF -->
      "isCustom": true
  }
  <!-- END entry_custom_column_group:loop -->
]
<!-- END_SetTemplate -->

